    ],
    
    "parameters": {
      "time_limit": {
        "type": "int",
        "description": "Maximum optimization time in seconds."
      }
    },
    "requirements": [
      "ortools",
      "qubots"
    ],
    "keywords": [
      "ortools",
      "constraints programming",
      "TSP solver",
      "exact"
    ]
}
  