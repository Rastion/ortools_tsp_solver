{
    "entry_point": "ortools_tsp_solver:ORToolsTSPSolver",
    "default_params": {
      "time_limit": 5
    },
    "creator": "Rastion",
    "type": "optimizer",
    "optimizer_name": "Constraints Programming TSP Solver",
